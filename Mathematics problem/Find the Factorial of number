//Find the Factroial of number
#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int fact(int num){
    if(num==0){
        return 1;
    }
    return num*fact(num-1);
}
int main() {
    int number;
    cout<<"the number is ";
    cin>>number;
    cout<<"The factroial of "<<number<<" is "<<fact(number);
    return 0;
}
