//Bitwise operator left shift(<<)
#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main(){
    int a,b;
    cout<<"The numbers are ";
    cin>>a>>b;
    cout<<"Left shift one time ";
    cout<<(a<<1)<<endl;
    cout<<"given b thime left shift ";
    cout<<(a<<b);
    return 0;
}
/*The numbers are 5
2
Left shift one time10
given b thime left shift20*/
