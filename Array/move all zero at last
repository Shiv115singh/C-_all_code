//move all zero at last
#include <iostream>
#include<bits/stdc++.h>
using namespace std;
void createarray(int arr[],int n){
    cout<<"Tipe the all emement of array";
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
}
void printarray(int arr[],int n){
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" "<<endl;
    }
}
int movezerolast(int arr[],int n){
    int count=0;
    for(int i=0;i<n;i++){
        if(arr[i]!=0){
            arr[count++]=arr[i];
        }
    }
    while(count<n){
        arr[count++]=0;
    }
  
   return n;
}

int main() {
    int N;
    cout<<"the length of array :- ";
    cin>>N;
    int arr[N];
    createarray(arr,N);
    cout<<"The given arrey is";
    printarray(arr,N);
    int n=movezerolast(arr,N);
    cout<<"after Removing Size of array :- "<<n<<endl;
    cout<<"The upadated array is :-";
    printarray(arr,n);

    return 0;
}
