#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main(){
    int a,k;
    cout<<"The numbers are ";
    cin>>a>>k;
    if(k==1){
        if(a%2==1){
            cout<<"YES";
        }else{
            cout<<"NO";
        }
    } else {
       a=(a>>(k-1));
        if(a%2==1){
            cout<<"YES";
        }else{
            cout<<"NO";
        }
    }
    return 0;
}
