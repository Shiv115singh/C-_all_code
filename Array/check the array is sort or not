#include<iostream>
#include<bits/stdc++.h>
using namespace std;
void insertelement(int arr[],int N){
    //Inserting the array Element
    for(int i=0;i<N;i++){
        cin>>arr[i];
    }
}

void checksort(int arr[],int n){
    
        for(int i=0;i<n-2;i++){
            if(arr[i]>arr[i+1]){
               cout<<"it is not sorted array";
            }
    }
    cout<<"it is sorted array";
}
int main(){
   int N;
   cout<<"The length of arrey is :-";
   cin>>N;
   int arr[N];
   cout<<"Inserting the array Element :- ";
   insertelement(arr,N);
   checksort(arr,N);
}
