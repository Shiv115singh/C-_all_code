//Find the Trailing zero in Factorial
#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int fact(int num){
    int res=0;
    for(int i=5;i<=num;i=i*5){
        res=res+(num/i);
    }
    return res;
}
int main() {
    int count=0;
    int number;
    cout<<"the number is ";
    cin>>number;
    cout<<fact(number);
}
