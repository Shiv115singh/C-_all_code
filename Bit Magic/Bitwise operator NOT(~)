//Bitwise operator NOT(~)
#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main(){
    signed int a,b;
    unsigned int c,d ;
    int e,f;
    cout<<"The numbers of signed integer are ";
    cin>>a>>b;
    cout<<(~a)<<" ";
    cout<<(~b)<<endl;
    cout<<"The numbers of unsigned integer are ";
    cin>>c>>d;
    cout<<(~c)<<" ";
    cout<<(~d)<<endl;
    cout<<"The numbers integer are ";
    cin>>e>>f;
    cout<<(~e)<<" ";
    cout<<(~f)<<endl;
    return 0;
}
/*The numbers of signed integer are 2
5
-3 -6
The numbers of unsigned integer are 2
5
4294967293 4294967290
The numbers integer are 2
5
-3 -6*/
