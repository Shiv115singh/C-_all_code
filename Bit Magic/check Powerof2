#include <iostream>
using namespace std;
   
void isPowerofTwo(long long n) {
        
        // Your code here
        if(n<=0)cout<<"false";
       if((n&(n-1))==0){
           cout<<"true";
       }else{
           cout<<"false";
       }
    }
int main() {
    int n;
    cout<<"the numbers is";
    cin>>n;
    isPowerofTwo(n);

    return 0;
}
