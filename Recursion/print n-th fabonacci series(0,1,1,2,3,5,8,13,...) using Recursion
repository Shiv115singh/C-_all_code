// print n-th fabonacci series(0,1,1,2,3,5,8,13,...) using Recursion
#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int febser(int n){
    if(n==0)return 0;
    if(n==1)return 1;
    return febser(n-1)+febser(n-2);
    
}
int main() {
    int n;
    cout<<"The number is ";
    cin>>n;
    cout<<febser(n);

    return 0;
}
