//check if kth bit is set or not
#include<iostream>
#include<bits/stdc++.h>
using namespace std;
void isset(int a,int k){
    int x=(a>>(k-1));
    if((x&1)!=0){
        cout<<"YES";
    }else{
        cout<<"NO";
    }
}
int main(){
    int a,k;
    cout<<"The numbers are ";
    cin>>a>>k;
    isset(a,k);
    return 0;
}
